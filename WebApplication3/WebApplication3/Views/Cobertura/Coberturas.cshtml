@model IEnumerable<WebApplication3.Models.Cobertura>

<p>
    @Html.ActionLink("Registrar Cobertura", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDCOBERTURA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAMOD)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDCOBERTURA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAMOD)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.IDCOBERTURA }) |
